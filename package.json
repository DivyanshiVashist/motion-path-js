{
  "name": "motion-path-js",
  "version": "0.0.0",
  "description": "Web Animations polyfill extension for motion path animation",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && karma start --single-run && npm run build",
    "watchTests": "karma start --reporters nyan",
    "build": "cat tools/pre.js tools/internal-scope.js $(cat src/fileOrder.txt) tools/post.js | uglifyjs > index.min.js",
    "lint": "semistandard src/*.js test/*.js",
    "format": "semistandard --fix src/*.js test/*.js"
  },
  "author": "",
  "license": "apache-2",
  "devDependencies": {
    "chai": "^3.5.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-nyan-reporter": "^0.2.4",
    "mocha": "^3.2.0",
    "semistandard": "~9.1.0",
    "uglifyjs": "^2.4.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Motion-Path-Polyfill/motion-path-js.git"
  }
}
